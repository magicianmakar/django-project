{
  "name": "shopifiedapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bower": "^1.8.8",
    "yuglify": "0.1.4"
  },
  "devDependencies": {
    "jshint": "^2.10.2",
    "jshint-stylish": "^2.2.1"
  },
  "scripts": {
    "postinstall": "bower install",
    "jshint": "scripts/jshint.sh",
    "server": "python3 manage.py runserver",
    "celery": "celery -A app.celery_base worker -Q celery,priority_high -O fair -l INFO"
  },
  "cacheDirectories": [
    "node_modules",
    "app/static/libs/bower_components"
  ],
  "engines": {
    "node": "14.x",
    "npm": "8.0.0"
  }
}
