#!/usr/bin/env bash

if [ ! "$DISABLE_COLLECTSTATIC" ]; then
    echo ' * Run python manage.py compress'
    python manage.py compress | grep -v /tmp/

    echo ' * Run python manage.py migrate --noinput'
    python manage.py migrate --noinput

    echo ' * Run manage.py migrate --database store_db --noinput'
    python manage.py migrate --database store_db --noinput

else
    echo " *** NOTE *** Escape Running:\n    > python manage.py compress\n    > python manage.py migrate --noinput"
fi
